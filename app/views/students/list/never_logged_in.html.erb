<% title "Never Logged In Students", false %>

<div class="row">
	<div class="col-md-12">
		<%= panel(title: "Never Logged In Students", icon: "users") do %>

	    <div class="row">
	      <div class="col-sm-12 col-xs-12">
	        <%= render "students/list/links" %>
	      </div>
	    </div>

			<hr/>
			

			<%= render "students/list", students: @students %>

	    <% if @students.size == 0 %>
	      <div class="student blank-slate">
	        <i class="fa fa-user"></i>
	        <h1>Students</h1>
	        <p>There are no never logged in students</p>
	      </div>
	    <% end %>

		<% end %>
	</div>
</div>
