<% title "Audit Trail / #{@student.name}", false %>
<div class="row">
	<div class="col-md-12">
		<%= panel(title: "Audit Trail / #{@student.name} ", icon: "list") do %>
			<div class="student-header row">
				<div class="col-md-10">
					<h4>
						<a class="btn btn-xs btn-outline-secondary" href="<%= student_path(@student) %>" title="Back To Student Details">
							<i class="fa fa-chevron-left"></i>
						</a>
						<%= @student.name %>
					</h4>
				</div>
				<div class="col-md-2">
					<h5>
						<a href="<%= audit_trail_student_path(@student, format: "xlsx") %>" class="pull-right">
							<i class="fa fa-table"></i> Download Exel
						</a>
					</h5>
				</div>
			</div>
			<hr/>
			<%= render partial:"audit_trail/trail", locals: {audits_grouped: @audits_grouped} %>
		<% end %>
	</div>
</div>
