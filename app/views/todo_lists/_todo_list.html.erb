<% if local_assigns[:show] == "tr" %>
  <%= render partial: "todo_lists/displays/tr", locals: { todo_list: todo_list } %>
<% elsif local_assigns[:show] == "li" %>
    <%= render partial: "todo_lists/displays/li", locals: { todo_list: todo_list } %>
<% elsif local_assigns[:show] == "div" %>
  <%= render partial: "todo_lists/displays/div", locals: { todo_list: todo_list } %>
<% else %>
  <%= render partial: "todo_lists/displays/public", locals: { todo_list: todo_list } %>
<% end %>
